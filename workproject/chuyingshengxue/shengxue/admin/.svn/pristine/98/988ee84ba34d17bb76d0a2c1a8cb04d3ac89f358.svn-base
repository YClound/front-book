/* */
.table-wrapper {
    position: relative;
    z-index: 10;
}
.inviter {
    height: 20px;
    position: relative;
}
.inviter .content {
    position: relative;
    position: absolute;
    width: 200px;
    top: -10px;
    left: -220px;
    border: 1px solid #D4D4D4;
    border-radius: 4px;
    line-height: 1.42858;
    background-color: #fff;
    padding: 10px;
    box-shadow: 5px 5px 10px rgba(0, 0, 0, .1);
    visibility: hidden;
    opacity: 0;
    filter: alpha(opacity=0);
    -webkit-transform: translateX(10px);
    transform: translateX(10px);
    -webkit-transition: all .4s;
    transition: all .4s
    z-index:11;
}
.inviter .content:before {
    content: '';
    position: absolute;
    border-left: 8px solid #D4D4D4;
    border-top: 8px solid transparent;
    border-bottom: 8px solid transparent
}
.inviter .content:after,
.inviter .content:before {
    left: 100%;
    top: auto;
    top: 8px
}

.inviter .content:after {
    content: '';
    position: absolute;
    border-left: 7px solid #fff;
    border-top: 7px solid transparent;
    border-bottom: 7px solid transparent;
    top: 9px
}

.inviter:hover .content {
    opacity: 1;
    filter: alpha(opacity=100);
    -webkit-transform: none;
    transform: none;
    visibility: visible
}


