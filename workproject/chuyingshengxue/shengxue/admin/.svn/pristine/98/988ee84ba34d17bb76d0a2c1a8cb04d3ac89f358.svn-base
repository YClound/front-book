.wu {}
.wu .file-item {
    width: 200px;
    position: relative;
    overflow: hidden;
}
.file-item .square {
    height: 200px;
}
.file-item .title {
    display: none;
}
.file-item .remove{
    font-size: 21px;
    line-height: 1;
    color: #000;
    text-shadow: 0 1px 0;
    opacity: .8;
    display: inline-block;
    padding: 0 3px;
}
.file-item .img-wrap img {
    width: 100%;
}
.file-item.state-progress:before{
    content: " ";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.5);
}
.file-item .float-bar {
    background-color: rgba(255,255,255,0.5);
    position: absolute;
    width: 100%;
    left: 0;
    top: 0;
    z-index: 10;
}
.file-item.state-progress .float-bar{
    display: none;
}
.file-item .progress {
    display: none;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    color: #FFFFFF;
}
.file-item.state-progress .progress{
    display: block;
}
.file-item .info {
    display: none;
    position: absolute;
    width: 100%;
    left: 0;
    bottom: 0;
    text-align: center;
    background: #f43838;
    color: #fff;
    height: 28px;
    line-height: 28px;
}
