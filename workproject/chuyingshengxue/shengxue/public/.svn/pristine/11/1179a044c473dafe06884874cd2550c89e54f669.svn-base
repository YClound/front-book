.main-container{
  width: 1190px;
  margin: 0 auto;
  padding-top: 20px;
  padding-bottom: 20px;
}
.paper-left{
  width: 250px;
  height: 270px;
  padding: 30px 20px;
  background-color: #fff;
  border-radius: 8px;
  margin-right: 20px;
}
.paper-left .make-paper-btn{
  display: block;
  width: 100%;
  border-radius: 20px;
  color: #fff;
  padding: 8px;
  margin-bottom: 20px;
  text-align: center;
}
.paper-left .user-info{
  padding: 0px 10px;
}
.paper-left .user-info img{margin-top: -4px;}
.paper-right{
  border-radius: 8px;
  background-color: #fff;
}
.paper-right .tab-title{
  text-align: center;
  border-bottom: #e6e6e6 1px solid;
  font-size: 24px;
  color: #000;
}
.tab-title-paper,.tab-title-question{
  width: 50%;
  position: relative;
  padding: 20px 0px;
  cursor: pointer;
}
.tab-title-paper.active, .tab-title-question.active{
  color: #2c82d1;
}
.tab-title-paper.active:after, .tab-title-question.active:after{
  content: "";
  position: absolute;
  bottom: 0px;
  left: 50%;
  shape-margin: -5px;
  border: transparent 10px solid;
  border-bottom: #e6e6e6 10px solid;
}
.tab-title-paper.active .paper-active, .tab-title-question.active .question-active{
  display: inline-block;
}
.tab-title-paper.active .paper-hide, .tab-title-question.active .question-hide{
  display: none;
}
.tab-title-paper .paper-active, .tab-title-question .question-active{
  display: none;
}
/*我的试卷*/
.tab-content{
  padding: 30px 20px;
}
.tab-content-paper .search-paper select{
  min-width: 200px;
  border-radius: 20px;
  padding: .6em .8em;
  border: #d9d9d9 1px solid;
  background-color: #fafafa;
}
.tab-content-paper .search-paper input[type="text"]{
  max-width: 120px;
  border-radius: 20px;
  border: #d9d9d9 1px solid;
  background-color: #fafafa;
}
.tab-content-paper .search-paper .search-paper-btn{
  margin-left: 20px;
  text-align: center;
  border-radius: 20px;
  border: 0px;
  padding: .6em 2.5em;
  color: #fff;
}
.search-paper .form-group{padding-left: 45px;}
/*试卷列表*/
.paper-list{
  padding-top: 30px;
  padding-bottom: 30px;
}
.paper-list .paper-list-header{
  text-align: center;
  border-radius: 50px;
  padding: 10px 0;
  background-color: #f2f2f2;
  margin-bottom: 30px;
}
.paper-list-content .paper-item{
  cursor: pointer;
  line-height: 25px;
  text-align: center;
  color: #808080;
  border-radius: 50px;
  padding: 10px 0px;
  border: transparent 1px solid;
  margin-bottom: 10px;
}
.paper-item .paper-print{
  display: inline-block;
  background-color: #f7f7f7;
  color: #cfcfcf;
  border-radius: 20px;
  padding: 0px 15px;
  margin-right: 10px;
  border: #d9d9d9 1px solid;
}
.paper-item .paper-del{
  display: inline-block;
  background-color: #f7f7f7;
  color: #cfcfcf;
  border-radius: 20px;
  padding: 0px 15px;
  border: #d9d9d9 1px solid;
}
.paper-item .paper-name{
  text-align: left;
  padding-left: 30px;
}
.paper-item:hover{
  border: #e5e5e5 1px solid;
  -webkit-box-shadow: 1px 1px 2px #e5e5e5,-1px -1px 2px #e5e5e5;
  -moz-box-shadow: 1px 1px 2px #e5e5e5,-1px -1px 2px #e5e5e5;
  -o-box-shadow: 1px 1px 2px #e5e5e5,-1px -1px 2px #e5e5e5;
  box-shadow: 1px 1px 2px #e5e5e5,-1px -1px 2px #e5e5e5;
}
.paper-item:hover .paper-name{
  color: #1f7ace;
}
.paper-item:hover .paper-print{
  background-color: #1f7ace;
  border-color: #1f7ace;
  color: #fff;
}
.paper-item:hover .paper-del{
  background-color: #f0a400;
  border-color: #f0a400;
  color: #fff;
}

/*我的题目*/
.question-filter .filter-type{
  display: inline-block;
  border-radius: 40px;
  padding: 6px 15px;
  margin: 0 5px;
  color: #1f7ace;
}
.question-filter .filter-type.active{
  background-color: #1f7ace;
  color: #fff;
}
.question-list{padding-top: 20px;}
.question-item{
  margin-bottom: 20px;
  border-radius: 8px;
  border: #cccccc 1px solid;
}
.question-item .question-header{
  font-size: 16px;
  background-color: #dbdbdb;
  font-weight: bold;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
  padding: 15px 20px;
}
.question-item .question-content{
  padding: 15px 20px;
}
.question-item .question-operat{
  line-height: 32px;
  padding: 0px 20px 20px;
}
.question-item .look-detail{
  display: inline-block;
  background-color: #1f7ace;
  color: #fff;
  padding: 0px 20px;
  border-radius: 20px;
}
.question-item:hover{
  -webkit-box-shadow: 1px 1px 3px #e5e5e5,-1px -1px 3px #e5e5e5;
  -moz-box-shadow: 1px 1px 3px #e5e5e5,-1px -1px 3px #e5e5e5;
  -o-box-shadow: 1px 1px 3px #e5e5e5,-1px -1px 3px #e5e5e5;
  box-shadow: 1px 1px 3px #e5e5e5,-1px -1px 3px #e5e5e5;
}