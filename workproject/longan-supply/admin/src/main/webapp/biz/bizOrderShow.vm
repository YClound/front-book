<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>订单详情</title>
	#parse("common/select.vm")
	#parse("common/pagination.vm")
	#parse("common/common.vm")
	
    <link rel="stylesheet" type="text/css" href="$!staticServer/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="$!staticServer/css/base.css" />
    <link rel="stylesheet" type="text/css" href="$!staticServer/css/list-common.css" />
	<link rel="stylesheet" type="text/css" href="$!staticServer/css/doubleDate.css" />
    <script type="text/javascript" src="$!staticServer/js/jquery-1.7.2.js"></script>
    <script type="text/javascript" src="$!staticServer/js/jquery-ui-1.8.22.custom.min.js"></script>
    <link rel="stylesheet" type="text/css" href="$!staticServer/css/ui-lightness/jquery-ui-1.8.22.custom.css" />
    <script type="text/javascript" src="$!staticServer/js/tip.js"></script>
	<script type="text/javascript" src="$!staticServer/js/doubleDate2.0.js"></script>
	<script type="text/javascript" src="$!staticServer/js/jquery.cookie.js"></script>
	
</head>
<body class="list-body">
<font color="#FF0000">$!errorMsg</font>
    <div class="list-container">
        <div class="alert-title fz-16">订单详情</div>
        <table class="table information-show table-condensed list">
                <thead>
                    <tr>
                        <td colspan="6">订单详情</td>
                    </tr>
                </thead>
                <tbody>
                     <tr>
                        <td class="w-8em">订单号</td>
                        <td class="detail">
                            $!bizOrder.id
                        </td>
                        <td class="w-8em">支付单号</td>
                        <td class="detail">
                            $!bizOrder.payOrderId
                        </td>
                        <td class="w-8em">下游流水号 </td>
                        <td class="detail">
                            $!bizOrder.downstreamSerialno
                        </td>
                    </tr>
                    <tr>
                        <td class="w-8em">商品</td>
                        <td class="detail">
                           $!bizOrder.itemName
                        </td>
                        <td class="w-8em">交易金额</td>
                        <td class="detail">
                            $!bizOrder.amountDesc
                        </td>
                        
                        #if($!bizOrder.bizId == 500)
                        <td class="w-8em">充值账号</td>
                        #else
                        <td class="w-8em">客户手机号</td>
                        #end
                        <td class="detail">
                            $!bizOrder.itemUid
                        </td>
                    </tr>
                    <tr>
                        <td class="w-8em">代理商</td>
                        <td class="detail">
                            $!bizOrder.downstreamName
                        </td>
                        <td class="w-8em">附加信息2</td>
                        <td class="detail">
                           $!bizOrder.itemExt2
                        </td>
                        <td class="w-8em">附加信息3</td>
                        <td class="detail">
                           $!bizOrder.itemExt3
                        </td>
                    </tr>     
                    <tr>
                        <td class="w-8em">交易时间 </td>
                        <td class="detail">
                           $!dateTool.format("yyyy-MM-dd HH:mm:ss", $!bizOrder.gmtCreate)
                        </td>
                        <td class="w-8em">订单状态</td>
                        <td class="detail">
                            $!bizOrder.statusDesc
                        </td>
                        <td class="w-8em">更改时间 </td>
                        <td class="detail">
                           $!dateTool.format("yyyy-MM-dd HH:mm:ss", $!bizOrder.gmtModify)
                        </td>
                    </tr>
                    <tr>
                        <td class="w-8em">供货方式</td>
                        <td class="detail">
                            $!traderInfo.supplyTypeDesc
                        </td>
                        <td class="w-8em">渠道</td>
                        <td class="detail">
                           $!bizOrder.channelDesc
                        </td>
                        #if($!bizOrder.bizId == 500)
                        <td class="w-8em">附加信息1</td>
                        #else
                        <td class="w-8em">pos机编号</td>
                        #end
                        <td class="detail">
                           $!bizOrder.itemExt1
                        </td>
                    </tr>
                    <tr>
                        <td class="w-8em">锁定人</td>
                        <td class="detail">
                           $!bizOrder.lockOperName
                        </td>
                        <td class="w-8em">处理人</td>
                        <td class="detail">
                            $!bizOrder.dealOperName
                        </td>
                        <td class="w-8em">库存编号</td>
                        <td class="detail">
                            $!bizOrder.stockId
                        </td>
                    </tr>
                    <tr>
                        <td class="w-8em">供货摘要</td>
                        <td class="detail">
                            $!bizOrder.upstreamMemo
                        </td>
                        <td class="w-8em">备注</td>
                        <td class="detail">
                           $!bizOrder.memo
                        </td>
                        <td class="w-8em">上游流水号</td>
                        <td class="detail">
                            $!bizOrder.upstreamSerialno
                        </td>
                    </tr>
                    <tr>
                        <td class="w-8em">上游编号</td>
                        <td class="detail">
                            $!bizOrder.upstreamId
                        </td>
                        <td class="w-8em">卡号</td>
                        <td class="detail">
                        $!bizOrder.itemCard
                        </td>
                        <td class="w-8em">上游商户名称</td>
                        <td class="detail">
                            $!traderUserInfo.userName
                        </td>
                    </tr>               
                    
                </tbody>
        </table>
    </div>
</body>