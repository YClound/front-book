	<update id="lockBizOrder" parameterClass="com.longan.biz.dataobject.BizOrder">
		update biz_order
		set
		gmt_modify = now(),lock_oper_id = #lockOperId#,status = 4,item_ext1 =
		#itemExt1#
		where id =
		#id# and status = 1
	</update>

	<update id="unLockBizOrder" parameterClass="com.longan.biz.dataobject.BizOrder">
		update biz_order
		set
		gmt_modify = now(),lock_oper_id = NULL,status = 1
		where id = #id# and
		status = 4
	</update>

	<update id="confirmBizOrder" parameterClass="com.longan.biz.dataobject.BizOrder">
		update biz_order
		set
		gmt_modify = now(),deal_oper_id = #dealOperId#,status = 2,cost_time =
		#costTime#,upstream_serialno=#upstreamSerialno#
		where id =
		#id# and
		(status = 9 or (status = 4 and
		lock_oper_id = #dealOperId#))
	</update>
	<update id="cancelBizOrder" parameterClass="com.longan.biz.dataobject.BizOrder">
		update biz_order
		set
		gmt_modify = now(),deal_oper_id = #dealOperId#,status = 3
		where id =
		#id# and (status = 1 or status = 9 or (status = 4 and lock_oper_id =
		#dealOperId#))
	</update>

	<update id="unConfirmBizOrder" parameterClass="com.longan.biz.dataobject.BizOrder">
		update biz_order
		set
		gmt_modify = now(),status = 9,item_card = #itemCard#,item_ext3 =
		#itemExt3#,upstream_serialno=#upstreamSerialno#
		where id =
		#id# and
		(status = 4 and
		lock_oper_id =
		#dealOperId#)
	</update>

